PROGRAM Hello
	BEGIN_VAR
		a: Integer;
		b: Integer;
	END_VAR
BEGIN
	b := 3 * (5 + 2) * 3;
	a := 3 * 5 + 2 * 3;
	WHILE b < a DO
		b := b + 1;
	END
	IF a < b THEN
		print(a);
	ELSE
		print(b);
	END
END
